---
title: Tatsuro Ninagawa • Hair stylist in London, UK
---

<main>

  <!-- <h1>
    Tatsuro Ninagawa
  </h1> -->

  <!-- <span id="loading-mask"></span> -->
  <img
    id="loading-mask"
    src="data:image/jpeg;base64,"
  >

  <video
    autoplay
    loop
    muted
    playsinline
    width="1920"
    height="1080"
    poster="/images/cutting-video-poster.jpg"
  >
    <source src="/videos/cutting-video-1280x720-1500k-streamable.mp4"  type="video/mp4" >
  </video>

</main>

<aside>
  <h1>
    Tatsuro <br>Ninagawa
  </h1>
  <img
    src="/images/icon-scissors-1x.png"
    width="20"
    height="20"
    srcset="/images/icon-scissors-1x.png 1x,
            /images/icon-scissors-2x.png 2x,
            /images/icon-scissors-3x.png 3x"
  >
  <p>
    <%= t(:intro) %>
  </p>
  <p>
    <a href="#bio" data-action="show-bio"><%= t(:label_show_bio) %></a>
  </p>
  <a
    class="button"
    href="http://instagram.com/i.am.tatsuro"
    target="_blank"
  >
    <img
      src="/images/icon-instagram-1x.png"
      width="20"
      height="20"
      srcset="/images/icon-instagram-1x.png 1x,
              /images/icon-instagram-2x.png 2x,
              /images/icon-instagram-3x.png 3x"
    >
    <%= t(:label_follow) %>
  </a>
  <a
    class="button"
    href="mailto:<%= t(:info_email_address) %>"
  >
    <img
      src="/images/icon-email-1x.png"
      width="20"
      height="20"
      srcset="/images/icon-email-1x.png 1x,
              /images/icon-email-2x.png 2x,
              /images/icon-email-3x.png 3x"
    >
    <%= t(:label_email) %>
  </a>
</aside>

<section id="about">

  <header>
    <button data-action="hide-bio"><%= t(:label_close) %></button>
  </header>

  <main>

    <article id="bio">
      <h2><%= t(:bio_title) %></h2>
      <p>
        <%= t(:bio_body_1) %>
      </p>
      <p>
        <%= t(:bio_body_2) %>
      </p>
      <p>
        <%= t(:bio_body_3) %>
      </p>
      <table>
        <tbody>
          <tr>
            <td><b><%= t(:label_location) %></b></td>
            <td>
              <a href="https://g.page/shunjilondon?share" target="blank">
                <%= t(:info_location) %>
              </a>
            </td>
          </tr>
          <tr>
            <td><b><%= t(:label_instagram_id) %></b></td>
            <td>
              <a href="http://instagram.com/<%= t(:info_instagram_id) %>" target="_blank">
                @<%= t(:info_instagram_id) %>
              </a>
            </td>
          </tr>
          <tr>
            <td><b><%= t(:label_email_address) %></b></td>
            <td>
              <a href="mailto:<%= t(:info_email_address) %>">
                <%= t(:info_email_address) %>
              </a>
            </td>
          </tr>
          <tr>
            <td><b><%= t(:label_line_id) %></b></td>
            <td>
              <a href="http://line.me/R/ti/p/~<%= t(:info_line_id) %>" target="_blank">
                <%= t(:info_line_id) %>
              </a>
            </td>
          </tr>
          <tr>
            <td><b><%= t(:label_phone_number) %></b></td>
            <td>
              <a href="tel:<%= t(:info_phone_number).gsub(/\s+/, '') %>">
                <%= t(:info_phone_number) %>
              </a>
            </td>
          </tr>
        </tbody>
      </table>
    </article>

